{% extends "base.html.twig" %}

{% block body %}
    <h3>Aktualnie w środku jest: {{ employeesIn }} osób</h3>

    {% if employeesIn > 0 %}
        <h3>Osoby w środku:</h3>
        {% for employee in employeeList %}
            <h4>{{ employee.employee_firstname }} {{ employee.employee_lastname }} - tel: {{ employee.employee_phone }} - data wejścia: {{ employee.entrance_date }}</h4>
            <hr>
        {% endfor %}
    {% endif %}
{% endblock %}